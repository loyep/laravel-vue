{
    "name": "grace",
    "private": true,
    "scripts": {
        "clean": "rm -rf ./public/{assets/app/*,js/*.{js,map},css/*.{css,map},images}",
        "clean:grace": "rm -rf ./public/{assets/admin/*,js/*.{js,map},css/*.{css,map},images}",
        "clean:all": "rm -rf ./public/{assets/*,js/*.{js,map},css/*.{css,map},images}",
        "dev": "npm run clean && npm run development",
        "dev:grace": "npm run clean:grace && npm run development -- --env.grace",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run dev -- --watch",
        "watch:grace": "npm run dev:grace -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run clean && npm run production",
        "prod:grace": "npm run clean:grace && npm run production -- --env.grace",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "dependencies": {
        "@antv/data-set": "^0.10.2",
        "ant-design-vue": "^1.3.5",
        "axios": "^0.18.0",
        "bootstrap": "^4.3.1",
        "enquire.js": "^2.1.6",
        "jquery": "^3.3.1",
        "js-cookie": "^2.2.0",
        "magnific-popup": "^1.1.0",
        "nprogress": "^0.2.0",
        "owl.carousel": "^2.3.4",
        "popper.js": "^1.14.7",
        "vue-clipboard2": "^0.3.0",
        "vue-i18n": "^8.8.2",
        "vue-ls": "^3.2.1",
        "vue-meta": "^1.5.8",
        "vue-router": "^3.0.2",
        "vuex": "^3.1.0",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "autoprefixer": "^8.6.5",
        "babel-eslint": "^10.0.1",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-plugin-import": "^1.11.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "cross-env": "^5.2.0",
        "eslint": "^5.14.1",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^5.2.2",
        "laravel-mix": "^4.0.14",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "lodash": "^4.17.11",
        "resolve-url-loader": "^3.0.1",
        "vue": "^2.6.7",
        "vue-template-compiler": "^2.6.7"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
