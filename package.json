{
    "name": "prism",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "clean": "rm -rf ./public/{static/app/*,js/*.{js,map},css/*.{css,map},images}",
        "clean:admin": "rm -rf ./public/{static/admin/*,js/*.{js,map},css/*.{css,map},images}",
        "clean:all": "rm -rf ./public/{static/*,js/*.{js,map},css/*.{css,map},images}",
        "dev": "npm run clean && npm run development --",
        "dev:admin": "npm run development -- --env.admin.js",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run dev -- --watch",
        "watch:admin": "npm run dev:admin -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run clean && npm run production",
        "prod:admin": "npm run production -- --env.admin.js && npm run production -- --env.admin.css",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "dependencies": {
        "@ant-design/icons": "~1.1.0",
        "@antv/data-set": "^0.10.2",
        "ant-design-vue": "^1.3.7",
        "axios": "^0.18.0",
        "enquire.js": "^2.1.6",
        "js-cookie": "^2.2.0",
        "nprogress": "^0.2.0",
        "vue-clipboard2": "^0.3.0",
        "vue-i18n": "^8.9.0",
        "vue-ls": "^3.2.1",
        "vue-meta": "^1.5.8",
        "vue-router": "^3.0.2",
        "vuex": "^3.1.0",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-syntax-jsx": "^7.2.0",
        "@types/node": "^11.12.0",
        "@types/nprogress": "0.0.29",
        "autoprefixer": "^8.6.5",
        "babel-helper-vue-jsx-merge-props": "^2.0.3",
        "babel-plugin-import": "^1.11.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "clean-webpack-plugin": "^2.0.1",
        "cross-env": "^5.2.0",
        "eslint": "^5.15.3",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^5.2.2",
        "happypack": "^5.0.1",
        "laravel-mix": "^4.0.15",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "lodash": "^4.17.11",
        "resolve-url-loader": "^3.0.1",
        "ts-loader": "^5.3.3",
        "tslint": "^5.14.0",
        "tslint-config-standard": "^8.0.1",
        "tslint-loader": "^3.5.4",
        "typescript": "^3.3.4000",
        "vue": "^2.6.10",
        "vue-class-component": "^7.0.2",
        "vue-property-decorator": "^8.1.0",
        "vue-template-compiler": "^2.6.10",
        "vuex-class": "^0.3.2"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
