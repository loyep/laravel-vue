        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^4.0.7",
        "lodash": "^4.17.5",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10",
        "webpack-livereload-plugin": "^1.2.0"
    },
    "dependencies": {
        "@ant-design/icons": "^2.0.0",
        "@antv/data-set": "^0.10.2",
        "animate.css": "^3.7.0",
        "ant-design-vue": "^1.3.8",
        "enquire.js": "^2.1.6",
